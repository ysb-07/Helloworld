package org;

public class DataTest {

	public static void main(String[] args) {
		//datatype  name of the variable
		//primitive types
				 //number type default int
				           byte b=1; // 8 bits
				           short s=12; // 16 bits
				           int x=122;  // 32 bits
				           long l=9034567118972765l; // 64 bits
				           
				           //Floating point default is double
				           float f= 1.345f; //32 bits
				           double d=12.55; //64 bits
				           
		 char ans= 'y';//16 bits 
		 boolean checked =true; //8 bits
		 
		 
	 //non-primitive type
		 String name ="reena";
				           
		 System.out.println(b);
		 System.out.println(s);
		 System.out.println(d);
		 System.out.println(l);
		 System.out.println(f);
		 System.out.println(d);
		 System.out.println(ans);
		 System.out.println(checked);
		 System.out.println(name);
		 
		           

	}

}
