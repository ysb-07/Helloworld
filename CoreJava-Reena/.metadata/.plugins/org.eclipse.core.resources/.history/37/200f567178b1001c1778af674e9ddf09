package switchExample;

public class SwitchTaxDeducationExample {

	public static void main(String[] args) {
		double salary = 600000;
		int category;
		double sat;
		if(salary <600000)
		{
			category = 10;
		}
		else if(salary <1000000)
		{
			category = 20;
		}
		else
		{
			category = 30;
		}
		switch(category) {
		case 10:
		
				sat = (salary*10)/100;
			System.out.println("you got salary after tax"+sat);
		    break;
		case 20:
			sat = (salary*20)/100;
			System.out.println("you got salary after tax"+sat);
		    break;
		case 30:
			sat = (salary*30)/100;
			System.out.println("you got salary after tax"+sat);
			break;
		default:
			System.out.println("No Tax deduction");

	}

}
}
